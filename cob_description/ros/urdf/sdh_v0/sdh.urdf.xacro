		<parent link="${name}_palm_link"/>
		<child link="${name}_finger_11_link"/>
		<axis xyz="0 0 1" />
                <dynamics damping="1" />
	        <calibration reference_position="0.0"/>
		<limit effort="100" lower="-2" upper="2" velocity="5"/> 
	</joint>

	<link name="${name}_finger_11_link">
